   "source": [
    "import pytesseract\n",
    "from gtts import gTTS  \n",
    "from PIL import Image\n",
    "\n",
    "picture = Image.open('pic.jpg')\n",
    "mytext = pytesseract.image_to_string(picture)\n",
    "language = 'en'\n",
    "myobject = gTTS(text=mytext, lang=language, slow=False) \n",
    "myobject.save(\"pic.mp3\")"
   ]
